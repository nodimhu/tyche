{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
